                var firstName = 'Jaya';
               var lastName = 'B';
               var prmId = page_type;
               //var prmId = '8832466522';
               var crmId = '';

              
				embedded_svc.settings.extraPrechatInfo = [{
					"entityFieldMaps": [{
						"doCreate":false,
						"doFind":false,
						"fieldName":"LastName",
						"isExactMatch":false,
						"label":"Last Name"
